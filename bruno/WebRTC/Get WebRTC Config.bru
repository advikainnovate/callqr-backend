meta {
  name: "Get WebRTC Config"
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/api/webrtc/config
  body: none
  auth: none
}

tests {
  test("Should return WebRTC configuration", function() {
    expect(res.status).to.equal(200);
    expect(res.body.success).to.be.true;
    expect(res.body.data.iceServers).to.be.an('array');
    expect(res.body.data.iceServers[0]).to.have.property('urls');
  });
}
